@model IEnumerable<GGZBTQPT_PRO.Models.T_XM_Financing>
@{
    ViewBag.Title = "发布的项目";
}
@*发布的项目信息*@
<div id="published">
    @foreach (var _model in Model)
    {
        <div class="row-fluid">
            @*项目信息*@
            <div class="published-content ">
                <h4>
                    <a href="#">@_model.ItemName</a></h4>
                @*摘要*@
                <p class="summary">
                    @_model.ItemContent
                </p>
                @*附属信息*@
                <div class="published-info">
                   <span class="pub-time">@_model.CreateTime</span><span class="pro-type">总投资：@_model.TotalInvestment</span><span class="favorite">多少人收藏</span>
                </div>
            </div>
        </div>
            
    }
    <div class="pagination pagination-left clear">
        <ul>
            <li class="disabled"><a href="#">Prev</a></li>
            <li class="active"><a href="#">1</a></li>
            <li><a href="#">2</a></li>
            <li><a href="#">3</a></li>
            <li><a href="#">4</a></li>
            <li><a href="#">5</a></li>
            <li><a href="#">6</a></li>
            <li><a href="#">7</a></li>
            <li class="disabled"><a href="#">Next</a></li>
        </ul>
    </div>
</div>
