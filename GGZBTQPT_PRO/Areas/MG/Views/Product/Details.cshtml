@model GGZBTQPT_PRO.Models.T_JG_Product

@{
    ViewBag.Title = "Details";
}

<h3>@Model.ProductName</h3>
<fieldset>
    <table>
            <tr>
                <td>产品名称</td>
                <td>@Html.DisplayFor(model => model.ProductName)</td>
            </tr>
            <tr>
                <td>所属机构</td>
                <td>@Html.DisplayFor(model => model.AgencyName)</td>
            </tr>
            <tr>
                <td>最高融资额度</td>
                <td>@Html.DisplayFor(model => model.FinancingAmount)（万元）</td>
            </tr>
            <tr>
                <td>融资期限</td>
                <td>@Html.DisplayFor(model => model.FinancingLimit)（月）</td>
            </tr>
            <tr>
                <td>利率</td>
                <td>@Html.DisplayFor(model => model.InterestRate)</td>
            </tr>
            <tr>
                <td>适用客户</td>
                <td>@Html.DisplayFor(model => model.CustomerType)</td>
            </tr>
            <tr>
                <td>产品优势</td>
                <td>@Html.DisplayFor(model => model.Superiority)</td>
            </tr>
            <tr>
                <td>还款方式</td>
                <td>@Html.DisplayFor(model => model.RepaymentType)</td>
            </tr>
            <tr>
                <td>申请条件</td>
                <td>@Html.DisplayFor(model => model.AppCondition)</td>
            </tr> 
            <tr>
                <td>办理流程</td>
                <td>@Html.DisplayFor(model => model.Process)</td>
            </tr>             
        </table> 
        <table>
            <tr>
                <td>联系人</td>
                <td>@Html.DisplayFor(model => model.Linkman)</td>
                <td>所属部门及职务</td>
                <td>@Html.DisplayFor(model => model.Position)</td>
            </tr>
            <tr>
                <td>固定电话</td>
                <td>@Html.DisplayFor(model => model.Phone)</td>
                <td>移动电话</td>
                <td>@Html.DisplayFor(model => model.Mobile)</td>
            </tr>
            <tr>
                <td>传真</td>
                <td>@Html.DisplayFor(model => model.Fax)</td>
                <td>电子邮箱</td>
                <td>@Html.DisplayFor(model => model.Email)</td>
            </tr>
        </table>   
</fieldset>
<p>
    @Html.ActionLink("返回", "Index")
</p>
