        @Ajax.ActionLink("发布的产品", "PublishedProducts", "Published", new { member_id = Model.ID }, new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "details", LoadingElementId = "Loading" })
    </li>*@

</ul>

<div id="details">

</div>



