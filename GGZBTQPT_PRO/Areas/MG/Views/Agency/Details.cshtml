@model GGZBTQPT_PRO.Models.T_JG_Agency

<fieldset>
    <legend>@Model.AgencyName</legend>
    <table style="width:100%">
            <tr>
                <td>机构简称</td>
                <td>@Html.DisplayFor(model => model.SubName)</td>
            </tr>
            <tr>
                <td style="width:150px">是否资本大厦入驻机构</td>
                <td>@(Model.IsIn ? "是" : "否")</td>
            </tr>
            <tr>
                <td>机构类别</td>
                <td>@Html.DisplayFor(model => model.AgencyTypeName)</td>
            </tr>
            <tr>
                <td>成立日期</td>
                <td>@Html.DisplayFor(model => model.RegTime)</td>
            </tr>
            <tr>
                <td>详细地址</td>
                <td>@Html.DisplayFor(model => model.Address)</td>
            </tr>
            <tr>
                <td>注册资本</td>
                <td>@Html.DisplayFor(model => model.RegCapital)（万元）</td>
            </tr>
            <tr>
                <td>工商注册地</td>
                <td>@Html.DisplayFor(model => model.AreaName)</td>
            </tr>
            <tr>
                <td>联系电话</td>
                <td>@Html.DisplayFor(model => model.Phone)</td>
            </tr>
            <tr>
                <td>机构服务内容</td>
                <td>@Html.DisplayFor(model => model.Services)</td>
            </tr>
            <tr>
                <td>机构简介</td>
                <td>@Html.Raw(Model.Remark)</td>
            </tr>
        </table> 
</fieldset>