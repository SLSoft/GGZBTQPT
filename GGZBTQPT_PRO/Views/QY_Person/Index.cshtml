@model IEnumerable<GGZBTQPT_PRO.Models.T_QY_Person>
@{
    Layout = null;
}
<div class="pageContent j-resizeGrid" style="border-left: 1px #B8D0D6 solid; border-right: 1px #B8D0D6 solid">
  <div class="panelBar">
    <ul class="toolBar">
      <li><a class="add" href="/QY_Person/create" rel="create" target="dialog" title="添加创业者" mask="true" width="600" height="600"><span>添加</span></a></li>
      <li><a class="edit" href="/QY_Person/edit/{mid}" rel="edit" target="dialog" title="编辑创业者信息" mask="true" warn="请选择一个创业者" width="600" height="600"><span>修改</span></a></li>
      <li><a class="delete" href="/QY_Person/delete/{mid}" rel="delete" target="ajaxTodo" title="确定要删除吗？"  warn="请选择一个创业者"><span>删除</span></a></li>
      <li class="line">line</li>
      <li><a class="icon" href="#"><span>导入EXCEL</span></a></li>
    </ul>
  </div>
  <div layouth="116">
    <table class="list" width="100%" asc="asc" desc="desc">
      <thead>
        <tr>
            <th>
                姓名
            </th>
            <th>
                性别
            </th>
            <th>
                出生日期
            </th>
            <th>
                证件号码
            </th>
    </tr>
      </thead>
      <tbody>
        @foreach (var item in Model)
        {
            <tr target="mid" rel=@item.ID>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Sex)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Birth)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CardID)
                </td>
            </tr>
        }
      </tbody>
    </table>
  </div>
  <div class="panelBar">
    <div class="pages">
      <span>显示</span>
      <select name="numPerPage" onchange="navTabPageBreak({numPerPage:this.value})">
        <option value="20">20</option>
        <option value="50">50</option>
        <option value="100">100</option>
        <option value="200">200</option>
      </select>
      <span>条，共23条</span>
    </div>
    <div class="pagination" targettype="navTab" totalcount="200" numperpage="20" pagenumshown="10" currentpage="2">
    </div>
  </div>
</div>
