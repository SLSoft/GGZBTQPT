@model IEnumerable<GGZBTQPT_PRO.ViewModels.VM_CPReport>
@{
    Layout = null;
}
<html>
<head>
    <title>注册创业者综合统计</title>
    <link rel="stylesheet" href="@Url.Content("~/Public/bootstrap/css/bootstrap.min.css")" type="text/css" media="screen" charset="utf-8">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/analysis.css")" type="text/css" media="screen" charset="utf-8">
    <link href="@Url.Content("~/Public/jqPlot/jquery.jqplot.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <!--[if lt IE 9]><script language="javascript" type="text/javascript" src="@Url.Content("~/Public/jqPlot/excanvas.min.js")"></script><![endif]-->
    <script src="@Url.Content("~/Public/jqPlot/jquery.jqplot.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Public/jqPlot/plugins/jqplot.pieRenderer.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Public/jqPlot/plugins/jqplot.donutRenderer.min.js")" type="text/javascript"></script>
    <style type="text/css">
    </style>
</head>
<body>
    <div class="container">
        <h1 class="center pdv20">
            注册创业者<strong class='lightblue'>综合汇总</strong>统计
        </h1>
        <div class="row-fluid">
            <p class="bg-lightgreen fnormal">
                截止到 @DateTime.Now.ToLongDateString(), 注册创业者共计 <em class="red fbig">@ViewBag.PersonCount</em>人，本年注册创业者<em class="red fbig">@ViewBag.YearSum</em>人，本月注册<em class="red fbig">@ViewBag.MountSum</em>人，今日注册<em class="red fbig">@ViewBag.DaySum</em>人</p>
        </div>
        @Html.Action("DataReportbyEducation")

        <h4 class="right under-line lightblue top-line pdv20">
            数据来源：截止到 @DateTime.Now.ToShortDateString() 注册创业者汇总数据
        </h4>
    </div>
</body>
</html>