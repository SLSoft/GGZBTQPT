@model GGZBTQPT_PRO.Models.T_QY_TZXQ

@{
    Layout = null;
}
<style type="text/css">
    .tb {width:100%; border:solid 1px #d0d0d0; border-collapse:collapse; }
    .tb td {padding:5px 5px;}
</style>
<div class="pageContent">
    
  <div layouth="1">
<table border="1px" cellpadding="0" cellspacing="0" class="tb">
        <tr>
            <td style="width:120px;">机构名称
            </td>
            <td>@Html.DisplayFor(model => model.AgencyName)
            </td>
        </tr>
        <tr>
            <td class="title">机构性质
            </td>
            <td>@Html.DisplayFor(model => model.AgencyNature)
            </td>
        </tr>
        <tr>
            <td class="title">机构地址
            </td>
            <td>@Html.DisplayFor(model => model.Address)
            </td>
        </tr>
        <tr>
            <td class="title">联系人
            </td>
            <td>@Html.DisplayFor(model => model.Linkman)
            </td>
        </tr>
        <tr>
            <td>职务
            </td>
            <td>@Html.DisplayFor(model => model.Duty)
            </td>
        </tr>
        <tr>
            <td>联系电话
            </td>
            <td>@Html.DisplayFor(model => model.Phone)
            </td>
        </tr>
        <tr>
            <td>Email
            </td>
            <td>@Html.DisplayFor(model => model.Email)
            </td>
        </tr>
        <tr>
            <td>过往投资总额
            </td>
            <td>@Html.DisplayFor(model => model.TotalInvestment)（万元）
            </td>
        </tr>
        <tr>
            <td>年均投项目数量
            </td>
            <td>@Html.DisplayFor(model => model.InvItemNum)
            </td>
        </tr>
        <tr>
            <td>最关注的领域
            </td>
            <td>@Html.DisplayFor(model => model.AttIndustry)
            </td>
        </tr>
        <tr>
            <td>关注企业类型
            </td>
            <td>@Html.DisplayFor(model => model.AttCorpType)@Html.DisplayFor(model => model.OtherCorpType)
            </td>
        </tr>
        <tr>
            <td>一般投资额度
            </td>
            <td>@Html.DisplayFor(model => model.Amount)（万元）
            </td>
        </tr>
        <tr>
            <td>期望回报率
            </td>
            <td>@Html.DisplayFor(model => model.RateOfReturn)（%/年）
            </td>
        </tr>
</table>
</div>
</div>