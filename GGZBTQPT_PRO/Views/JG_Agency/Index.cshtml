@model IEnumerable<GGZBTQPT_PRO.Models.T_JG_Agency>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            机构名称
        </th>
        <th>
            机构类别
        </th>
        <th>
            联系电话
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AgencyName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AgencyType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegTime,"成立日期")
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone,"联系电话")
        </td>
        <td>
            @Html.ActionLink("编辑", "Edit", new { id=item.ID }) |
            @Html.ActionLink("查看", "Details", new { id=item.ID }) |
            @Html.ActionLink("删除", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
