@{
    Layout = null;
}
@model IEnumerable<GGZBTQPT_PRO.Models.T_XM_Financing>
<!DOCTYPE HTML>
<html>
<head>
    <title>融资项目综合统计</title>
    <link rel="stylesheet" href="@Url.Content("~/Public/bootstrap/css/bootstrap.min.css")" type="text/css" media="screen" charset="utf-8">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/analysis.css")" type="text/css" media="screen" charset="utf-8">
    <link href="@Url.Content("~/Public/jqPlot/jquery.jqplot.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <!--[if lt IE 9]><script language="javascript" type="text/javascript" src="@Url.Content("~/Public/jqPlot/excanvas.min.js")"></script><![endif]-->
    <script src="@Url.Content("~/Public/jqPlot/jquery.jqplot.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Public/jqPlot/plugins/jqplot.barRenderer.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Public/jqPlot/plugins/jqplot.categoryAxisRenderer.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Public/jqPlot/plugins/jqplot.pointLabels.min.js")" type="text/javascript"></script>
    <style type="text/css">
    </style>
</head>
<body>
    <div class="container">
        <h1 class="center pdv20">
            融资项目<strong class='lightblue'>综合汇总</strong>统计
        </h1>
        <div class="row-fluid">
            <p class="bg-lightgreen fnormal">
                截止到 @DateTime.Now.ToLongDateString(), 登记融资项目共计 <em class="red fbig">@ViewBag.RZXMCount</em>个</p>
        </div>
        @Html.Action("DataReportbyIndustry")

        <h4 class="right under-line lightblue top-line pdv20">
            数据来源：截止到 @DateTime.Now.ToShortDateString() 融资项目汇总数据
        </h4>
    </div>
</body>
</html>