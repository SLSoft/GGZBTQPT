        <table>
            <tr>
                <td>资产类别</td>
                <td>@Html.DisplayFor(model => model.AssetsType)</td>
            </tr>
            <tr>
                <td>资产估价</td>
                <td>@Html.DisplayFor(model => model.AssetsCost)</td>
            </tr>
            <tr>
                <td>转让价格</td>
                <td>@Html.DisplayFor(model => model.TransferPrice)</td>
            </tr>
            <tr>
                <td>是否抵质押</td>
