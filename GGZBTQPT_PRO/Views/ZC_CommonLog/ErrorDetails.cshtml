@model GGZBTQPT_PRO.Models.T_ZC_CommonLog

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Details</title>
</head>
<body>
    <fieldset>
        <legend>T_ZC_CommonLog</legend>
    
        <div class="display-label">Date</div>
        <div class="display-field">
            @Html.DisplayFor(model => model.Date)
        </div> 

    
        <div class="display-label">来源</div>
        <div class="display-field">
            @Html.DisplayFor(model => model.Logger)
        </div>
        <div class="display-label">行数</div>
        <div class="display-field">
            @Html.DisplayFor(model => model.Source)
        </div>
        <div class="display-label">日志记录信息</div>
        <div class="display-field">
            @Html.DisplayFor(model => model.Message)
        </div> 
    
        <div class="display-label">详细堆栈信息</div>
        <div class="display-field">
            @Html.DisplayFor(model => model.Exception)
        </div>
    
        <div class="display-label">操作人</div>
        <div class="display-field">
            @Html.DisplayFor(model => model.User)
        </div>
    </fieldset> 
</body>
</html>
